    "@trpc/server": "^10.18.0",
    "api": "^0.1.0",
    "auth": "^0.1.0",
    "db": "^0.1.0",
    "next": "^13.2.4",
    "next-auth": "^4.20.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "superjson": "1.9.1",
    "tailwind-config": "^0.1.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.31",
    "@types/react-dom": "^18.0.11",
    "autoprefixer": "^10.4.14",
    "dotenv-cli": "^7.1.0",
    "eslint": "^8.37.0",
    "eslint-config": "^0.1.0",
